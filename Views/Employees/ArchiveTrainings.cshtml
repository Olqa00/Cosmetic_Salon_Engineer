                                                </svg>
                                                @sharedResource["Participants"]
                                            </div>
                                            <div class="col-auto">
                                                @(training.Users?.Count() ?? 0)  / @training.UsersNumber
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-4">
                                                <svg class="me-2" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 21 19" fill="none">
                                                    <path d="M10.4892 18C15.4379 18 19.4496 14.1944 19.4496 9.5C19.4496 4.80558 15.4379 1 10.4892 1C5.54052 1 1.52881 4.80558 1.52881 9.5C1.52881 14.1944 5.54052 18 10.4892 18Z" stroke="#631E1E" stroke-width="1.5" />
                                                    <path d="M10.4893 4.81036V9.50002L12.9611 11.8448" stroke="#631E1E" stroke-width="1.5" stroke-linecap="round" />
                                                </svg>
                                                @sharedResource["Duration"]

                                            </div>
                                            <div class="col-auto">
                                                @training.Duration min
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-4">
                                                <svg class="me-2" xmlns="http://www.w3.org/2000/svg" width="17" height="20" viewBox="0 0 17 20" fill="none">
                                                    <path d="M15.3777 3.48279H5.07234C3.17516 3.48279 1.63721 4.94172 1.63721 6.74141C1.63721 8.54104 3.17516 10 5.07234 10H11.9426C13.8397 10 15.3777 11.459 15.3777 13.2587C15.3777 15.0583 13.8397 16.5173 11.9426 16.5173H1.63721" stroke="#631E1E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                                                    <path d="M8.18018 19V1" stroke="#631E1E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                                                </svg>
                                                @sharedResource["Cost"]
                                            </div>
                                            <div class="col-auto">
                                                @training.Price Euro
                                            </div>
                                        </div>
                                    </div>
                                    <div class="buttons col-md-5">
                                        <a asp-controller="Trainings" asp-action="Delete" asp-route-id="@training.Id" class="buttonDelete btn btn-info">@sharedResource["Delete"]</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal fade" id="treatmentModal-@training.Id" tabindex="-1" aria-labelledby="exampleModalLabel " aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="exampleModalLabel">@sharedResource["HaveBeenChanges"]</h5>
                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                </div>
                                <div class="modal-body">
                                    @if (training != null)
                                    {
                                        <form asp-action="ArchiveTrainings" method="post">
                                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                            <input type="hidden" asp-for="@training.Id" />
                                            <a>@sharedResource["DidTrainingNotPerformed"]</a>

                                            <div class="form-group">
                                                <input type="submit" value='@sharedResource["Save"]' class="button btn btn-info" />
                                            </div>
                                        </form>
                                    }

                                </div>
                            </div>
                        </div>
                    </div>
                }
                @Html.Partial("_TrainingsPartial", new List<Training>())
            </div>
            <div class="button-container mt-4">
                <button id="load-more-button" class="button btn btn-info">@sharedResource["LoadMoreTrainings"]</button>
            </div>
        </div>
    </div>
    <script>
        filledstars();
        function filledstars() {
            const filledstars = document.querySelectorAll('.filled-stars-container');
            for (let x = 0; x < filledstars.length; x++) {
                const stars = filledstars[x].querySelectorAll('svg');
                const treatmentId = filledstars[x].dataset.treatmentId;

                $.ajax({
                    type: 'GET',
                    url: '/Trainings/GetAverageRating',
                    data: {
                        treatmentId: treatmentId
                    },
                    dataType: 'json',
                    success: function (data) {
                        const rating = data.averageRating;
                        const starsCount = rating;
                        for (let z = 0; z < starsCount; z++) {
                            stars[z].classList.add('active');
                        }
                    },
                    error: function (error) {
                        console.error('Error getting average rating:', error);
                    }
                });
            }
        }
        let skip = 10;

        document.getElementById('load-more-button').addEventListener('click', function () {
            loadTrainings(skip);
        });

        function loadTrainings(skip) {

            $.ajax({
                url: '/Employee/LoadTrainingsArchive',
                type: 'GET',
                data: { skip: skip },
                success: function (data) {

                    $('#treatment-table').append(data);
                    skip += 10;

                    skipTrainings(skip);
                    filledstars();
                }
            });
        }
        function skipTrainings(updatedSkip) {
            skip = updatedSkip;
        }
    </script>
</main>