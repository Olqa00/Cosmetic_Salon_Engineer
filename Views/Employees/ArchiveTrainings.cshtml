                                                @sharedResource["Employee"]
                                            </div>
                                            <div class="col-auto">
                                                @training.Employee?.FullName
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-4">
                                                <svg class="me-2" style="margin-left:4px;" xmlns="http://www.w3.org/2000/svg" width="13" height="20" viewBox="0 0 13 20" fill="none">
